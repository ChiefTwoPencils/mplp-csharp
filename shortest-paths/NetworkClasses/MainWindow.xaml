<Window x:Class="NetworkClasses.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetworkClasses"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="statusLabel" Content="Shortest Paths"></Label>
        <TextBox x:Name="netTextBox" Margin="0,31,0,-31"></TextBox>
        <Button x:Name="one" Content="1" HorizontalAlignment="Left" Margin="658,6,0,0" VerticalAlignment="Top" Width="33" Click="one_Click"/>
        <Button x:Name="two" Content="2" HorizontalAlignment="Left" Margin="701,6,0,0" VerticalAlignment="Top" Width="33" Click="two_Click"/>
        <Button x:Name="three" Content="3" HorizontalAlignment="Left" Margin="744,6,0,0" VerticalAlignment="Top" Width="33" Click="three_Click"/>        
    </Grid>
</Window>
